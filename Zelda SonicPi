    play :d5
    sleep 1
    play :a4
    sleep 2
    play :g4
    sleep 0.5
    play :a4
    sleep 0.5
    play :b4
    sleep 2
    play :d5
    sleep 1
    play :a4
    sleep 3
    play :b4
    sleep 2
    play :d5
    sleep 1
    play :a5
    sleep 2
    play :g5
    sleep 1
    play :d5
    sleep 2
